&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="c"># Get the data&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">&lt;span class="k">foreach&lt;/span> &lt;span class="p">(&lt;/span>&lt;span class="nv">$subscription_Id&lt;/span> &lt;span class="k">in&lt;/span> &lt;span class="nv">$subscription_Ids&lt;/span>&lt;span class="p">)&lt;/span> &lt;span class="p">{&lt;/span>
